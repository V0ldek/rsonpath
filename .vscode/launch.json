    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rsonpath'",
            "env": {
                "RUSTFLAGS": "-C target-cpu=native"
            },
            "cargo": {
                "args": [
                    "build",
                    "--bin=rsonpath",
                    "--no-default-features"
                ],
                "filter": {
                    "name": "rsonpath",
                    "kind": "bin"
                }
            },
            "args": [
                "$..a..b.c..d",
                "./data/basic_compressed/child.json",
                "-v"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'rsonpath'",
            "env": {
                "RUSTFLAGS": "-C target-feature=+avx2"
            },
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=rsonpath",
                ]
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'rsonpath'",
            "env": {
                "RUSTFLAGS": "-C target-feature=+avx2"
            },
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=rsonpath",
                    "--package=rsonpath"
                ]
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug integration tests in 'count_correctness_test'",
            "env": {
                "RUSTFLAGS": "-C target-feature=+avx2"
            },
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--package=rsonpath",
                    "--test=count_correctness_test"
                ],
                "filter": {
                    "name": "count_correctness_test",
                    "kind": "test"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}/rsonpath"
        }
    ]
}